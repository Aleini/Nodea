{
	"name": "newmipsWorkspace",
	"description": "A newmips generated workspace",
	"repository": "https://github.com/newmips/newmips",
	"README": "https://github.com/newmips/newmips/blob/master/README.md",
	"license": "GPL-3.0",
	"main": "server.js",
	"scripts": {
		"test": "jest --detectOpenHandles",
		"lint": "reset;eslint ./;",
		"jsdoc_core": "./node_modules/.bin/jsdoc ./_core --recurse -c ./jsdoc.conf.json -d docs_core -t ./node_modules/foodoc/template",
		"jsdoc_app": "./node_modules/.bin/jsdoc ./app --recurse -c ./jsdoc.conf.json -d docs_app -t ./node_modules/foodoc/template"
	},
	"jest": {
		"moduleNameMapper": {
			"@core(.*)": "<rootDir>/_core$1",
			"@app(.*)": "<rootDir>/app$1",
			"@config(.*)": "<rootDir>/config$1"
		}
	},
	"dependencies": {
		"@jest-mock/express": "^1.4.5",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.18.3",
		"connect-flash": "~0.1.1",
		"connect-pg-simple": "^6.2.1",
		"console-stamp": "^3.0.0-rc4.2",
		"consolidate": "^0.16.0",
		"cookie-parser": "~1.4.3",
		"decompress": "^4.2.1",
		"docxtemplater": "^3.1.12",
		"dustjs-helpers": "~1.7.3",
		"dustjs-linkedin": "^2.7.5",
		"express": "^4.16.3",
		"express-mysql-session": "^2.1.5",
		"express-session": "^1.15.6",
		"express-socket.io-session": "^1.3.2",
		"fill-pdf": "1.1.0",
		"fs-extra": "^9.0.1",
		"hat": "0.0.3",
		"html-pdf": "^2.2.0",
		"jest": "^27.2.0",
		"jimp": "^0.16.1",
		"jsonschema": "^1.2.4",
		"jszip": "^2",
		"mariadb": "^2.5.3",
		"matomo-tracker": "^2.2.1",
		"mime-types": "^2.1.17",
		"module-alias": "^2.2.2",
		"moment": "^2.22.1",
		"moment-timezone": "^0.5.31",
		"morgan": "^1.9.0",
		"multer": "^1.2.0",
		"mysql": "^2.15.0",
		"mysql2": "^2.2.5",
		"nodemailer": "^6.4.17",
		"ovh": "^2.0.1",
		"passport": "^0.4.0",
		"passport-local": "~1.0.0",
		"pg": "^8.5.1",
		"public-holidays": "^0.2.2",
		"randomstring": "^1.1.5",
		"response-time": "^2.3.2",
		"sequelize": "^6.3.5",
		"socket.io": "^3.0.5",
		"split": "^1.0.0",
		"svg-captcha": "^1.3.12",
		"uuid": "^8.3.2",
		"winston": "^3.3.3",
		"zip-local": "^0.3.4"
	},
	"devDependencies": {
		"eslint": "^7.10.0",
		"foodoc": "0.0.9",
		"jsdoc": "^3.6.6"
	},
	"_moduleAliases": {
		"@core": "_core",
		"@app": "app",
		"@config": "config"
	}
}
