{
	"appname": "nodea_workspace",
	"version": "1.0.0",
	"maintenance": false,
	"api_enabled": true,
	"lang": "fr-FR",
	"timezone": "Europe/Paris",
	"contact_field_for_sms": "f_phone",
	"socket": {
		"enabled": true,
		"chat": false,
		"notification": true
	},
	"document_template": {
		"format_pairs": [{
			"code": "docx_docx",
			"label": "global_component.document_template.format_pairs.docx_docx",
			"input": "docx",
			"output": "docx"
		}, {
			"code": "dust_pdf",
			"label": "global_component.document_template.format_pairs.dust_pdf",
			"input": "dust",
			"output": "pdf"
		}],
		"input_extentions": ["pdf", "dust", "html", "doc", "docx"],
		"output_extentions": ["docx", "pdf", "html"]
	},
	"synchronization": {
		"ignore_list": [
			"e_action",
			"e_api_credentials",
			"e_group",
			"e_role",
			"e_media_function",
			"e_media_mail",
			"e_media_notification",
			"e_notification",
			"e_status",
			"e_synchronization",
			"e_synchro_credentials",
			"e_translation",
			"e_media_sms",
			"e_user"
		]
	},
	"resizePicture": {
		"enabled": false,
		"width": 400,
		"height": 400,
		"quality": 100
	},
	"thumbnail": {
		"folder": "thumbnail/",
		"width": 30,
		"height": 30,
		"quality": 60
	}
}
