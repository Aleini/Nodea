<section id='section_e_address_E_ENTITY_fields' class='col-12 section_address_fields'>
	<div data-field='address_search_input' class='col-12 div_address_search_input'>
		<div class='form-group'>
			<div class='input-group'>
				<div class='input-group-prepend'>
					<span class='input-group-text'>
						<i class='fa fa-map-marker' aria-hidden='true'></i>
					</span>
				</div>
				<div class='input-group-prepend'>
					<button type='button' class='input-group-text clear-address-search'>
						<i class='fa fa-times' style='color: red;'></i>
					</button>
				</div>
				<input class='input form-control address_search_input' id='address_search_input'  type='text' name='f_address_label' placeholder='{#__ key="entity.e_address_E_ENTITY.search"/}' >
			</div>
		</div>
		<hr>
	</div>
	<div data-field='f_address_housenumber' class='col-12' style='display:block'>
		<div class='form-group'>
			<label for='f_address_housenumber' class=''>{#__ key="entity.e_address_E_ENTITY.f_address_housenumber"/}</label>
			<input class='input form-control address_field f_address_housenumber '   field='housenumber'    type='string' placeholder='{#__ key="entity.e_address_E_ENTITY.f_address_housenumber"/}' name='f_address_housenumber' id='f_address_housenumber'>
		</div>
	</div>
	<div data-field='f_address_street' class='col-12' style='display:block'>
		<div class='form-group'>
			<label for='f_address_street' class=''>{#__ key="entity.e_address_E_ENTITY.f_address_street"/}</label>
			<input class='input form-control address_field f_address_street '   field='street'    type='text' placeholder='{#__ key="entity.e_address_E_ENTITY.f_address_street"/}' name='f_address_street' id='f_address_street'>
		</div>
	</div>
	<div data-field='f_address_complement1' class='col-12' style='display:block'>
		<div class='form-group'>
			<label for='f_address_complement1' class=''>{#__ key="entity.e_address_E_ENTITY.f_address_complement1"/}</label>
