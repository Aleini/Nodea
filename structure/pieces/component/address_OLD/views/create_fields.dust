			<input class='input form-control address_field f_address_postcode '  maxlength="5" field='postcode' pattern="[0-9]{5}"   type='text' placeholder='{#__ key="entity.e_address_E_ENTITY.f_address_postcode"/}' name='f_address_postcode' id='f_address_postcode'>
		</div>
	</div>
	<div data-field='f_address_city' class='col-12' style='display:block'>
		<div class='form-group'>
			<label for='f_address_city' class=''>{#__ key="entity.e_address_E_ENTITY.f_address_city"/}</label>
